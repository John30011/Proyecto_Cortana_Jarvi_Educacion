# URL base de la API
VITE_API_URL=http://localhost:8000/api

# Configuración de Supabase
VITE_SUPABASE_URL=tu_url_de_supabase
VITE_SUPABASE_ANON_KEY=tu_clave_anonima

# Configuración de la aplicación
VITE_APP_NAME=GEMINI
VITE_APP_ENV=development
VITE_APP_VERSION=0.1.0

# Configuración de autenticación
VITE_AUTH_ENABLED=true
VITE_AUTH_DEBUG=false
VITE_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Configuración de OAuth
VITE_GOOGLE_OAUTH_CLIENT_ID=tu_google_client_id
VITE_GITHUB_OAUTH_CLIENT_ID=tu_github_client_id

# Configuración de Google Analytics (opcional)
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Configuración de Sentry (opcional)
# VITE_SENTRY_DSN=your-sentry-dsn-here

# Configuración de Feature Flags (opcional)
# VITE_FEATURE_CHAT=true
# VITE_FEATURE_GAMES=true
# VITE_FEATURE_PARENT_DASHBOARD=true
